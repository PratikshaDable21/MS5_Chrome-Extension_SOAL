{
    "name": "Today's Weather" ,
    "manifest_version": 2,
    "version": "1.0.0",
    "browser_action": {
        "default_title": "Today's Weather",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],   
"permissions": ["tabs"]
}
